package com.edaisong.api.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.edaisong.api.business.SqlSessionFactoryPool;
import com.edaisong.api.dal.model.BusinessBalanceRecord;
import com.edaisong.api.service.inter.ITestService;




public class Hellp {

	public static void main(String[] args) {
		ITestService testService =SqlSessionFactoryPool.getCustomBeanByType(ITestService.class);

		List<BusinessBalanceRecord> listData=testService.selectBusinessBalanceByID(9,"2015-1-1");
		 for (int i = 0; i < listData.size(); i++) {
			 System.out.println(listData.get(i).getId());
			 System.out.println(listData.get(i).getBusinessId());
			 System.out.println(listData.get(i).getOperator());
			 System.out.println(listData.get(i).getRecordType());
			 System.out.println(listData.get(i).getRelationNo());
			 System.out.println(listData.get(i).getRemark());
			 System.out.println(listData.get(i).getStatus());
			 System.out.println(listData.get(i).getWithwardId());
			 System.out.println(listData.get(i).getAmount());
			 System.out.println(listData.get(i).getBalance());
			 System.out.println(listData.get(i).getOperateTime());
		 }
		
		
		testService.addBusinessBalance(new BusinessBalanceRecord());
		testService.deleteBusinessBalance(1541);
		testService.updateBusinessBalance(new BusinessBalanceRecord());
    	System.exit(0);
	}
}
